{
    "version": 2,
    "name": "Full Stack",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["prisma/schema.prisma", "prisma/migrations/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
